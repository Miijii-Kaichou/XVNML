        <keycode name::"_navRight"             vkey::Right          purpose::NAVIGATION_RIGHT/>
    </keycodeDefinitions>

    <macroCache>
        <macro #uint name::"_3sec" arg::3000 symbol::"delay"/>
        <macro #uint name::"_defaultTextSpeed" arg::100 symbol::"set_text_speed"/>
    </macroCache>

	<dialogue name::"Hello World">
        ["Main"]
        @ {$_3sec|insert::"Wowwie"|$_defaultTextSpeed|pass}<
